{
	"easycom": {
		"custom": {
			"^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"tabBar": {
		"color": "#999999",
		"selectedColor": "#00D465",
		"borderStyle": "black",
		"backgroundColor": "#ffffff00",
		"list": [{
				"pagePath": "pages/TabBar/Home/Home",
				"iconPath": "static/tabbar/home.png",
				"selectedIconPath": "static/tabbar/homeSeleted.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/TabBar/Stores/Stores",
				"iconPath": "static/tabbar/shop.png",
				"selectedIconPath": "static/tabbar/shopSeleted.png",
				"text": "商城"
			},
			{
				"pagePath": "pages/TabBar/Shopping/Shopping",
				"iconPath": "static/tabbar/cart.png",
				"selectedIconPath": "static/tabbar/cartSeleted.png",
				"text": "购物车"
			},
			{
				"pagePath": "pages/TabBar/Order/Order",
				"iconPath": "static/tabbar/order.png",
				"selectedIconPath": "static/tabbar/orderSeleted.png",
				"text": "订单"
			},
			{
				"pagePath": "pages/TabBar/Mine/Mine",
				"iconPath": "static/tabbar/my.png",
				"selectedIconPath": "static/tabbar/mySeleted.png",
				"text": "我的"
			}
		]
	},

	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/TabBar/Home/Home",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}

		}, {
			"path": "pages/TabBar/Stores/Stores",
			"style": {
				"navigationBarTitleText": "商城",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}

		}, {
			"path": "pages/TabBar/Shopping/Shopping",
			"style": {
				"navigationBarTitleText": "购物车",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}

		}, {
			"path": "pages/TabBar/Order/Order",
			"style": {
				"navigationBarTitleText": "订单",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}

		}, {
			"path": "pages/TabBar/Mine/Mine",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}

		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#D1D3EC",
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom"
	},
	"uniIdRouter": {}
	// "window":{
	// 	"navigationStyle": "custom"
	// }
}